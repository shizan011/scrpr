{
    "version":2,
    "builds": [
        {
            "src": "email_scraper/wsgi.py",
            "use": "@vercel/python",
            "config":{ "maxLamdaSize":"15mb","runtime":"python3.11.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{ "distDir":"static"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "email_scraper/wsgi.py"
        }
    ]
}